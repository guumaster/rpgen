{
  "name": "@guumaster/rpgen",
  "version": "1.1.0",
  "description": "Another RPG random generator engine",
  "author": "guumaster <guuweb@gmail.com>",
  "main": "lib/rpgen.min.js",
  "unpkg": "lib/rpgen.min.js",
  "browser": "lib/rpgen.min.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/guumaster/rpgen"
  },
  "files": [
    "src/",
    "lib/rpgen.min.js"
  ],
  "scripts": {
    "build": "NODE_ENV=production WEBPACK_ENV=build webpack --progress --hide-modules --colors",
    "postbuild": "npm run build:examples",
    "build:dev": "NODE_ENV=development WEBPACK_ENV=build webpack --progress --hide-modules --colors",
    "build:examples": "cp lib/rpgen.min.js examples/",
    "dev": "WEBPACK_ENV=dev webpack --progress --hide-modules --colors --watch",
    "lint": "eslint src",
    "prepublish": "npm run build"
  },
  "license": "GPL",
  "dependencies": {
    "axios": "0.16.1",
    "lodash.merge": "4.6.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-preset-env": "1.4.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "lodash-webpack-plugin": "0.11.2",
    "webpack": "2.4.1"
  }
}
